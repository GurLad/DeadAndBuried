; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DeadAndBuried"
config/features=PackedStringArray("4.4", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

CemeteryLoader="*res://Scenes/Loaders/CemeteryLoader.tscn"
CoffinIconLoader="*res://Scenes/Loaders/CoffinIconLoader.tscn"
GraveIconLoader="*res://Scenes/Loaders/GraveIconLoader.tscn"
LevelGeneratorLoader="*res://Scenes/Loaders/LevelGeneratorLoader.tscn"
PersonLoader="*res://Scenes/Loaders/PersonLoader.tscn"

[dotnet]

project/assembly_name="DeadAndBuried"

[file_customization]

folder_colors={
"res://Scenes/": "purple",
"res://Scenes/Loaders/": "pink",
"res://Scenes/UI/": "red",
"res://Scripts/": "orange",
"res://Scripts/Data/": "yellow",
"res://Scripts/GodotUtils/": "green",
"res://Scripts/Loaders/": "teal",
"res://Scripts/Logic/": "blue",
"res://Scripts/UI/": "purple",
"res://Sprites/": "pink",
